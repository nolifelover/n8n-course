# PostgreSQL Configuration
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=n8n

# n8n Configuration
N8N_ENCRYPTION_KEY=your_encryption_key
N8N_USER_MANAGEMENT_JWT_SECRET=your_jwt_secret
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true

# Webhook URL
WEBHOOK_URL=https://your-domain.com
SSL_EMAIL=your-email@example.com

# Email Configuration
N8N_EMAIL_MODE=smtp
N8N_SMTP_HOST=your_smtp_host
N8N_SMTP_USER=your_smtp_user
N8N_SMTP_SENDER=your_smtp_sender
N8N_SMTP_PASS=your_smtp_password
N8N_SMTP_PORT=587
N8N_SMTP_SSL=false

# Timezone
GENERIC_TIMEZONE=Asia/Bangkok

# UI
VUE_APP_URL_BASE_API=https://your-domain.com/

# Execution Settings
N8N_DEFAULT_EXECUTION_MODE=queue
EXECUTIONS_PROCESS=main
EXECUTIONS_MODE=queue
EXECUTIONS_TIMEOUT=3600  # 1 hour timeout for long-running workflows

# Logging Configuration
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console,file
N8N_LOG_FILE=/var/log/n8n.log 